{
  "name": "pit-web-generator",
  "displayName": "pit-web-generator",
  "description": "pit-web-generator",
  "publisher": "heqiang",
  "version": "1.0.9",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:pit-web-generator.webGenerator"
  ],
  "main": "./out/build.js",
  "contributes": {
    "commands": [
      {
        "command": "pit-web-generator.webGenerator",
        "title": "webGenerator"
      }
    ],
    "configuration": [
      {
        "type": "object",
        "title": "pit-web-generator.webGeneratorConfig configuration",
        "properties": {
          "pit-web-generator.webGeneratorConfig": {
            "type": "object",
            "default": {
              "connect": {
                "host": "",
                "port": "",
                "user": "",
                "password": ""
              },
              "componentList": []
            },
            "description": "pit-web-generator.webGeneratorConfig configuration."
          }
        }
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "esbuild-base": "esbuild extension.js --bundle --outfile=out/build.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@vscode/test-electron": "^2.1.5",
    "esbuild": "^0.15.8",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "ejs": "^3.1.8",
    "mysql": "^2.18.1"
  }
}
